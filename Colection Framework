import java.util.*;

public class CollectionExamples {
    public static void main(String[] args) {
        // ArrayList (List)
        List<String> names = new ArrayList<>();
        names.add("Alice");
        names.add("Bob");
        names.add("Alice"); // Duplicates allowed
        System.out.println("ArrayList: " + names);

        // HashSet (Set)
        Set<String> uniqueNames = new HashSet<>();
        uniqueNames.add("Alice");
        uniqueNames.add("Bob");
        uniqueNames.add("Alice"); // Duplicate ignored
        System.out.println("HashSet: " + uniqueNames);

        // HashMap (Map)
        Map<String, Integer> ages = new HashMap<>();
        ages.put("Alice", 30);
        ages.put("Bob", 25);
        System.out.println("HashMap: " + ages);


        // LinkedList (List and Queue)
        Queue<Integer> queue = new LinkedList<>();
        queue.offer(1);
        queue.offer(2);
        queue.offer(3);
        System.out.println("Queue (LinkedList): " + queue);
        System.out.println("Removing from Queue: " + queue.poll()); //Removes and returns the head of the queue


        // TreeSet (Set)
        Set<Integer> sortedNumbers = new TreeSet<>();
        sortedNumbers.add(5);
        sortedNumbers.add(2);
        sortedNumbers.add(8);
        System.out.println("TreeSet (Sorted): " + sortedNumbers);
    }
}
